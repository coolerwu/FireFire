{
  "name": "firefire",
  "version": "0.6.8",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@tiptap/core": "^2.0.2",
    "@tiptap/extension-bubble-menu": "^2.0.2",
    "@tiptap/extension-character-count": "^2.0.2",
    "@tiptap/extension-code-block-lowlight": "^2.0.2",
    "@tiptap/extension-color": "^2.0.2",
    "@tiptap/extension-document": "^2.0.2",
    "@tiptap/extension-dropcursor": "^2.0.2",
    "@tiptap/extension-floating-menu": "^2.0.2",
    "@tiptap/extension-highlight": "^2.0.2",
    "@tiptap/extension-horizontal-rule": "^2.0.2",
    "@tiptap/extension-image": "^2.0.2",
    "@tiptap/extension-link": "^2.0.2",
    "@tiptap/extension-paragraph": "^2.0.2",
    "@tiptap/extension-placeholder": "^2.0.2",
    "@tiptap/extension-table": "^2.0.2",
    "@tiptap/extension-table-cell": "^2.0.2",
    "@tiptap/extension-table-header": "^2.0.2",
    "@tiptap/extension-table-row": "^2.0.2",
    "@tiptap/extension-task-item": "^2.0.2",
    "@tiptap/extension-task-list": "^2.0.2",
    "@tiptap/extension-text-align": "^2.0.2",
    "@tiptap/extension-text-style": "^2.0.2",
    "@tiptap/extension-typography": "^2.0.2",
    "@tiptap/extension-underline": "^2.0.2",
    "@tiptap/react": "^2.0.2",
    "@tiptap/starter-kit": "^2.0.2",
    "@tiptap/suggestion": "^2.27.1",
    "antd": "^5.4.0",
    "autoprefixer": "^10.4.22",
    "babel-plugin-import": "^1.13.5",
    "better-sqlite3": "^11.10.0",
    "customize-cra": "^1.0.0",
    "dayjs": "^1.11.19",
    "dompurify": "^3.3.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "gray-matter": "^4.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lowlight": "^2.7.0",
    "marked": "^15.0.12",
    "moment": "^2.29.4",
    "postcss": "^8.5.6",
    "prebuild-install": "^7.1.2",
    "prosemirror-commands": "^1.3.0",
    "prosemirror-example-setup": "^1.2.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-markdown": "^1.9.3",
    "prosemirror-model": "^1.18.1",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-state": "^1.4.1",
    "prosemirror-view": "^1.27.1",
    "react": "^18.3.1",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.3.1",
    "react-pdf": "^7.7.3",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.4.18",
    "tippy.js": "^6.3.7",
    "turndown": "^7.1.1",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "estart": "electron .",
    "all": "npm run build && npm run estart",
    "estart-dev": "electron . dev",
    "postinstall": "node scripts/postinstall.js",
    "rebuild": "npx @electron/rebuild",
    "rebuild-native": "electron-rebuild -f -w better-sqlite3",
    "package-mac": "electron-builder --mac --publish=never",
    "package-mac-arm64": "electron-builder --mac --arm64 --publish=never",
    "package-mac-x64": "electron-builder --mac --x64 --publish=never",
    "package-win": "electron-builder --win --x64 --publish=never",
    "package-linux": "electron-builder --linux --x64 --publish=never",
    "publish-mac": "electron-builder --mac --x64 --publish=always",
    "publish-win": "electron-builder --win --x64 --publish=always",
    "publish-linux": "electron-builder --linux --x64 --publish=always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coolerwu/FireFire.git"
  },
  "build": {
    "appId": "org.firefire.app",
    "productName": "firefire",
    "copyright": "CopyRight @2022-2025 CoolerWu firefire",
    "directories": {
      "output": "target"
    },
    "files": [
      "build/**/*",
      "electron/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "extraMetadata": {
      "main": "main.js"
    },
    "asarUnpack": [],
    "npmRebuild": true,
    "buildDependenciesFromSource": true,
    "publish": {
      "provider": "github",
      "owner": "coolerwu",
      "repo": "FireFire"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.productivity",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb",
      "maintainer": "CoolerWu",
      "vendor": "CoolerWu",
      "description": "firefire"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron/rebuild": "^3.7.2",
    "electron": "^25.9.8",
    "electron-builder": "^24.6.4"
  }
}

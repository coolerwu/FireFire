  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "estart": "electron .",
    "all": "cnpm run build && cnpm run estart",
    "estart-dev": "electron . dev",
    "package-mac": "electron-builder --mac --x64 --publish=never",
    "package-win": "electron-builder --win --x64 --publish=never",
    "package-linux": "electron-builder --linux --x64 --publish=never"
  },
  "build": {
    "appId": "org.firefire.app",
    "productName": "firefire",
    "copyright": "CopyRight @2022-2023 CoolerWu firefire",
    "directories": {
      "output": "target"
    },
    "files": [
      "build/**/*",
      "electron/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "extraMetadata": {
      "main": "main.js"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "appImage"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "dmg-license": "^1.0.11",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3"
  }
